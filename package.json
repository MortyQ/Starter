    "pinia": "^2.1.0",
    "vue": "^3.4.0",
    "vue-multiselect": "^3.4.0",
    "vue-router": "^4.2.0",
    "vue-sonner": "^2.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@iconify/json": "^2.2.397",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/dompurify": "^3.2.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
