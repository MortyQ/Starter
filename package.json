    "tsx": "^4.20.3",
    "typescript": "^5.2.0",
    "unplugin-icons": "^22.4.2",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^5.0.0",
    "vue-eslint-parser": "^10.2.0",
